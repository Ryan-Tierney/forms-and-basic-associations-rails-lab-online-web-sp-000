<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %> 
  <%= f.collection_select :genre_name, Genre.all :name, :name %>
  <%= f.text_field :song_notes_1 %>
  <%= f.text_field :song_notes_2 %> 
  
  
  <br />

  <%= f.submit %>
<% end %>


