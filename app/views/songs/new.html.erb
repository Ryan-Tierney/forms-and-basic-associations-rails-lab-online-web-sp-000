<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_name, Genre.all :name, :name %><br>
  <%= f.text_field :song_notes_1 %><br>
  <%= f.text_field :song_notes_2 %><br> 
  
  
  <br />

  <%= f.submit %>
<% end %>


